{
  "private": false,
  "main": "index.js",
  "version": "1.0.0",
  "name": "@levakorg/prettier-config",
  "description": "Prettier config levakorg org's",
  "author": "levakorg authors <levakorg@gmail.com>",
  "homepage": "https://github.com/levakorg/prettier-config",
  "repository": "https://github.com/levakorg/prettier-config",
  "bugs": "https://github.com/levakorg/prettier-config/issues",
  "license": "MIT",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "TODO.md",
    "CHANGELOG.md",
    "index.js",
    ".prettierrc.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prettier": "prettier check .",
    "prettier:fix": "prettier --write .",
    "eslint": "eslint . --ext .json,.cjs,.mjs,.js,.ts,.jsx,.tsx,.vue,.svelte",
    "eslint:fix": "eslint . --ext .json,.cjs,.mjs,.js,.ts,.jsx,.tsx,.vue,.svelte --fix",
    "format": "yarn prettier:fix && yarn eslint:fix"
  },
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.1"
  },
  "devDependencies": {
    "@levakorg/eslint-config": "^1.0.0",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1"
  },
  "peerDependencies": {
    "prettier": "^2.7.1"
  }
}
